{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/odata/v2/basic/(.*)$",
      "target": "/$1",
      "destination": "SF_1",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/odata/v2/sf2/(.*)$",
      "target": "/$1",
      "destination": "SF_2",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/odata/v2/sf3/(.*)$",
      "target": "/$1",
      "destination": "SF_3",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/odata/v2/sf4/(.*)$",
      "target": "/$1",
      "destination": "SF_4",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/odata/v2/sf5/(.*)$",
      "target": "/$1",
      "destination": "SF_5",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/odata/v2/oauth/(.*)$",
      "target": "/$1",
      "destination": "SF_OAUTH",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/mail/(.*)$",
      "target": "/$1",
      "destination": "psn_mail",
      "authenticationType": "basic"
    },   
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}